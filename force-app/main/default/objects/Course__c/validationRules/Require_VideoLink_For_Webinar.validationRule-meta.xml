<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Require_VideoLink_For_Webinar</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISPICKVAL(Type__c, &quot;Webinar&quot;),
    ISBLANK(VideoLink__c)
)</errorConditionFormula>
    <errorMessage>Digital møte lenke kan ikke være tom når kurstypen er webinar.</errorMessage>
</ValidationRule>
