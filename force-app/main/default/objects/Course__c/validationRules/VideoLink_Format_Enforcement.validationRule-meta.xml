<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VideoLink_Format_Enforcement</fullName>
    <active>true</active>
    <description>Triggers when missing &quot;https://www.&quot; format</description>
    <errorConditionFormula>AND(
    NOT(ISBLANK(VideoLink__c)),
    NOT(CONTAINS(VideoLink__c, &quot;https://www.&quot;))
)</errorConditionFormula>
    <errorMessage>Feltet digital møte lenke må begynne med «https://www.» for å være gyldig. For eksempel: &quot;https://www.nav.no/&quot;</errorMessage>
</ValidationRule>
